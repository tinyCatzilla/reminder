{
    "manifest_version": 2,
    "name": "Reminder Extension",
    "version": "1.0",
    "description": "A Firefox extension for setting reminders.",
    "permissions": ["alarms", "notifications"],
    "background": {
      "scripts": ["background.js"]
    },
    "options_ui": {
      "page": "options/options.html"
    },
    "browser_action": {
      "default_popup": "popup/popup.html"
    }
  }
  